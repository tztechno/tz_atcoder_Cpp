#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    int N;
    std::cin >> N;
    std::vector<int> A(N);

    for (int i = 0; i < N; i++) {
        std::cin >> A[i];
    }

    std::sort(A.begin(), A.end());

    if (std::unique(A.begin(), A.end()) == A.end()) {
        std::cout << "Yes" << std::endl;
    } else {
        std::cout << "No" << std::endl;
    }

    return 0;
}

#########################

N=int(input())
A=list(map(int,input().split()))
if len(set(A))==1:
  print('Yes')
else:
  print('No')
